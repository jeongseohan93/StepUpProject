<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>

	<typeAliases>
		<typeAlias type="vo.AcademyVO" alias = "academyVO" />
		<typeAlias type="vo.RatingVO" alias = "ratingVO" />
		<typeAlias type="vo.InstructorVO" alias = "instructorVO" />
		<typeAlias type="vo.ScheduleVO" alias = "scheduleVO" />
		<typeAlias type="vo.AcademyReviewVO" alias = "academyreviewVO" />
		<typeAlias type="vo.BoardVO" alias="boardVO"/>
		<typeAlias type="vo.CommentVO" alias="commentVO"/>
		<typeAlias type="vo.StepupMemberVO" alias="stepupmemberVO"/>
		<typeAlias type="vo.LoginVO" alias="loginVO"/>
		<typeAlias type="vo.ResumeVO" alias="resumeVO"/>
		<typeAlias type="vo.CartVO" alias="cartVO"/>
		<typeAlias type="vo.CurseVO" alias="curseVO"/>
		<typeAlias type="vo.PicVO" alias="picVO"/>

	</typeAliases>

	<mappers>
		<mapper resource="config/mybatis/mapper/academy.xml" />
		<mapper resource="config/mybatis/mapper/instructor.xml" />
		<mapper resource="config/mybatis/mapper/schedule.xml" />
		<mapper resource="config/mybatis/mapper/board.xml" />
		<mapper resource="config/mybatis/mapper/comment.xml" />
		<mapper resource="config/mybatis/mapper/login.xml" />
		<mapper resource="config/mybatis/mapper/memberjoin.xml" />
		<mapper resource="config/mybatis/mapper/information.xml" />
		<mapper resource="config/mybatis/mapper/cart.xml" />
	</mappers>

</configuration>



